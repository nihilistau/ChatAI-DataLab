{
  "name": "playground-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "playground:dev": "vite --port 5174 --open control-center.html",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit --pretty",
    "check": "npm run lint && npm run typecheck",
    "preview": "vite preview",
    "playground:preview": "vite preview --open /control-center.html",
    "test": "vitest run",
    "test:playground": "vitest run src/control-center/__tests__/ControlCenterApp.test.tsx",
    "test:watch": "vitest",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:playground": "storybook build --config-dir .storybook-playground --output-dir storybook-static-playground",
    "chromatic": "chromatic --config ./chromatic.config.mjs"
  },
  "dependencies": {
    "@xyflow/react": "^12.0.1",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.0.7",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
  "@vitejs/plugin-react": "^5.1.1",
    "chromatic": "^13.3.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.0.7",
    "eslint-plugin-testing-library": "^7.13.4",
    "eslint-plugin-vitest": "^0.5.4",
  "happy-dom": "^20.0.10",
    "storybook": "^10.0.7",
    "typescript": "^5.5.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  }
}
